{
  "expo": {
    "name": "Skybuddy",
    "slug": "Skybuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/coolimage.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ddbdd5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Skybuddy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Nunito-Black.ttf"
          ]
        }
      ]
    ]
  }
}
